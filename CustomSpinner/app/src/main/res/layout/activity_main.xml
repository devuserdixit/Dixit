        android:layout_height="wrap_content"/>

    <Spinner
        android:id="@+id/Spinner5"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Spinner
        android:id="@+id/Spinner6"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_width="match_parent"
